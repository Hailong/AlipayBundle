    "type": "symfony-bundle",
    "keywords": ["symfony", "alipay", "sdk"],
    "license": "MIT",
    "homepage": "https://github.com/Hailong/AlipayBundle",
    "authors": [
        {
            "name": "Hailong Zhao",
            "email": "hailongzh@hotmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Orinoco\\AlipayBundle\\": "AlipayBundle"
